{
  "ignorePatterns": [
    {
      "pattern": "^http://127.0.0.1"
    },
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://github.com/.*/pull/"
    },
    {
      "pattern": "^https://github.com/.*/issues/"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com/"],
      "headers": {
        "Accept": "text/html"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206, 301, 302, 307, 308]
}
